{% extends "layout.html.twig" %}

{% block body %}
  {% for feed in feeds %}
    {% if feed.reposted != 1 %}
      <img class="lazyload" data-sizes="auto" data-src="{{ feed.url }}" style="max-width: 1000px;"/>
      <legend style="font-size: 1em;">
        <strong>ID :</strong>&nbsp;{{ feed.id }} |
        <strong>Url :</strong>&nbsp;<a href="{{ feed.url }}">Here</a><br/>
        <strong>Hash :</strong>&nbsp;{{ feed.hash }} |
        <strong>Offset :</strong>&nbsp;{{ feed.offset }}<br/>
        <strong>Enabled :</strong>&nbsp;{{ feed.enabled == 1 ? 'Yes' : 'No' }} |
        <strong>Viewed :</strong>&nbsp;{{ feed.enabled == 1? 'Yes' : 'No' }} |
        <strong>Approved :</strong>&nbsp;{{ feed.enabled == 1? 'Yes' : 'No' }}<br/>
        {% if feed.comment != '' %}
          <strong>Comment :</strong><br/>{{ feed.comment  }}<br/>
        {% endif %}
      </legend>
    {% endif %}
  {% endfor %}
{% endblock %}
